{
  "name": "sortingtest",
  "version": "1.0.0",
  "description": "Простой Express сервер на TypeScript",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:nodemon": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:bcrypt --external:mongodb",
    "build:clean": "rm -rf dist && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --external:bcrypt --external:mongodb",
    "watch": "tsc -w",
    "devs": "nodemon --exec ts-node src/index.ts",
    "heroku-postbuild": "yarn build",
    "test": "jest --detectOpenHandles --no-cache",
    "sorting": "ts-node src/models/sorting.ts",
    "test:mongo": "ts-node src/testMongo.ts"
  },
  "dependencies": {
    "@types/jest": "^30.0.0",
    "@types/nodemailer": "^7.0.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "jest": "^30.2.0",
    "jquery": "^3.7.1",
    "jsonwebtoken": "^9.0.3",
    "mailtrap": "^4.4.0",
    "mongodb": "^7.0.0",
    "nodemailer": "^8.0.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^25.0.3",
    "esbuild": "^0.27.2",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
